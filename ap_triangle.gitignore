import math

def triangle(self, a, b, c):
  self.a=a
  self.b=b
  self.c=c
  
  def area(self):
    s=(self.a+self.b+self.c)/2
    area=math.sqrt(s(s-self.a)(s-self.b)*(s-self.c))
    return area

  def perimeter(self):
    perimeter = a + b + c
    return perimeter
    
t1 = triangle(1, 2, 3)
t1.area()
